{
  "prettier.enable": false,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": "always",
    "source.fixAll.eslint": "always"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.associations": {
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.wts": "typescript",
    "bitset": "cpp",
    "variant": "cpp",
    "ios": "cpp",
    "__availability": "cpp",
    "string": "cpp",
    "stdexcept": "cpp",
    "__node_handle": "cpp",
    "optional": "cpp",
    "regex": "cpp",
    "sstream": "cpp",
    "__bit_reference": "cpp",
    "deque": "cpp",
    "__memory": "cpp",
    "limits": "cpp",
    "locale": "cpp",
    "ratio": "cpp",
    "tuple": "cpp",
    "vector": "cpp",
    "__hash_table": "cpp",
    "__tree": "cpp",
    "array": "cpp",
    "type_traits": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "list": "cpp",
    "__config": "cpp",
    "__locale": "cpp",
    "__split_buffer": "cpp",
    "__threading_support": "cpp",
    "__verbose_abort": "cpp",
    "cctype": "cpp",
    "charconv": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "complex": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "execution": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "map": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "queue": "cpp",
    "set": "cpp",
    "stack": "cpp",
    "streambuf": "cpp",
    "string_view": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "algorithm": "cpp",
    "memory": "cpp",
    "functional": "cpp"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.autoGuessEncoding": true,
}